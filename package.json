{
  "name": "github-action-typescript-template",
  "version": "0.0.0",
  "private": true,
  "description": "A template for creating a GitHub Action written in TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint --fix src/**/*.ts",
    "lint:check": "eslint src/**/*.ts",
    "build": "ncc build src/main.ts --license licenses.txt",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelsoncanarinho/typescript-action"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Cloud Posse LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "1.10.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "18.19.21",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.2",
    "typescript": "4.9.5"
  }
}
